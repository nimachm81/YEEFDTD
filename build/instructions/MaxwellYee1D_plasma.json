
{
"simulationType":"singleGrid",
"simulationParameters":{
    "variables":["_z0_", "_z1_", "_nz_", "_dz_", "_dt_", "_m_dt_", "_nt_", "_z_j_", "_indJ_", "_indJ_p1_", "_dt_dz_", "_m_dt_dz_", "_wp_sq_", "_cube_z0_", "_cube_z1_", "_cube_dz_", "_m_dt_tau_"],
    "dimensions":{
        "r0":[0.0, 0.0, "_z0_"],
        "r1":[0.1, 0.1, "_z1_"],
        "nCells":[1, 1, "_nz_"],
        "dt":"_dt_"
    },
    "entireGridArrays":[{"name":"E", "type":"EdgeE"}, {"name":"H", "type":"EdgeH"}, {"name":"Jp", "type":"EdgeE"}, {"name":"Wp_sq", "type":"EdgeE"}],
    "partialGridArrays":[{"name":"J", "type":"EdgeE", "indStart":[0, 0, "_indJ_"], "nCells":[1, 1, 0]}],
    "girdArrayManipulators":[
        {
        "type":"GaussianGridArrayManipulator",
        "parameters":{
            "name":"Jupdater",
            "array":"J",
            "direction":"x",
            "amplitude":1.0,
            "t_center":3.0,
            "t_decay":0.5,
            "modulationFrequency":1.8,
            "modulationPhase":0.0,
            "timeOffsetFraction":-0.5
            }
        },
        {
        "type":"SpatialCubeGridArrayManipulator",
        "parameters":{
            "name":"WpUpdater",
            "array":"Wp_sq",
            "direction":"x",
            "cornerR0":[-0.1, -0.1, "_cube_z0_"],
            "cornerR1":[0.2, 0.2, "_cube_z1_"],
            "edgeThickness":[0.0, 0.0, "_cube_dz_"],
            "valueInside":"_wp_sq_",
            "valueOutside":0.0
            }
        }
    ],
    "updateInstructions":[
        {
        "type":"A+=sumbC",
        "parameters":{
            "name":"E_pe_curl_H",
            "A":"E",
            "A_direction":"x",
            "A_indStart":[0, 0, 1],
            "A_indEnd":[1, 1, "_nz_"],
            "b":["_dt_dz_", "_m_dt_dz_"],
            "C":["H", "H"],
            "C_direction":["y", "y"],
            "C_indStart":[[0, 0, 0], [0, 0, 1]]
            }
        },
        {
        "type":"A+=sumbBC",
        "parameters":{
            "name":"Jp_pe_WpsqE",
            "A":"Jp",
            "A_direction":"x",
            "A_indStart":[0, 0, 1],
            "A_indEnd":[1, 1, "_nz_"],
            "b":["_dt_"],
            "B":["Wp_sq"],
            "B_direction":["x"],
            "B_indStart":[[0, 0, 1]],
            "C":["E"],
            "C_direction":["x"],
            "C_indStart":[[0, 0, 1]]
            }
        },
        {
        "type":"A+=sumbC",
        "parameters":{
            "name":"Jp_me_gammaJp",
            "A":"Jp",
            "A_direction":"x",
            "A_indStart":[0, 0, 0],
            "A_indEnd":[1, 1, "_nz_"],
            "b":["_m_dt_tau_"],
            "C":["Jp"],
            "C_direction":["x"],
            "C_indStart":[[0, 0, 0]]
            }
        },
        {
        "type":"A+=sumbC",
        "parameters":{
            "name":"H_pe_m_curl_E",
            "A":"H",
            "A_direction":"y",
            "A_indStart":[0, 0, 0],
            "A_indEnd":[1, 1, "_nz_"],
            "b":["_dt_dz_", "_m_dt_dz_"],
            "C":["E", "E"],
            "C_direction":["x", "x"],
            "C_indStart":[[0, 0, 0], [0, 0, 1]]
            }
        },
        {
        "type":"A+=sumbC",
        "parameters":{
            "name":"E_me_J",
            "A":"E",
            "A_direction":"x",
            "A_indStart":[0, 0, "_indJ_"],
            "A_indEnd":[1, 1, "_indJ_p1_"],
            "b":["_m_dt_dz_"],
            "C":["J"],
            "C_direction":["x"],
            "C_indStart":[[0, 0, 0]]
            }
        },
        {
        "type":"A+=sumbC",
        "parameters":{
            "name":"E_me_Jp",
            "A":"E",
            "A_direction":"x",
            "A_indStart":[0, 0, 0],
            "A_indEnd":[1, 1, "_nz_"],
            "b":["_m_dt_"],
            "C":["Jp"],
            "C_direction":["x"],
            "C_indStart":[[0, 0, 0]]
            }
        },
        {
        "type":"A=frt",
        "parameters":{
            "name":"J_update",
            "girdArrayManipulator":"Jupdater"
            }
        },
        {
        "type":"A=frt",
        "parameters":{
            "name":"Wp_update",
            "girdArrayManipulator":"WpUpdater"
            }
        }
    ],
    "updateSequences":[{"name":"materialSetup", "sequence":["Wp_update"]},
                       {"name":"iterative", "sequence":["J_update", "E_me_J", "E_me_Jp", "E_pe_curl_H", "H_pe_m_curl_E", "Jp_pe_WpsqE", "Jp_me_gammaJp"]}],
    "runSequence":[{"name":"materialSetup", "timeIndStart":0, "timeIndEnd":1},
                   {"name":"iterative", "timeIndStart":0, "timeIndEnd":"_nt_"}],
    "gridViews":[
        {
        "type":"entire",
        "parameters":{
            "name":"E-x",
            "array":"E",
            "direction":"x",
            "saveRate":5
            }
        },
        {
        "type":"entire",
        "parameters":{
            "name":"H-y",
            "array":"H",
            "direction":"y",
            "saveRate":1
            }
        },
        {
        "type":"entire",
        "parameters":{
            "name":"J-x",
            "array":"J",
            "direction":"x",
            "saveRate":1
            }
        }
    ]
}
}


