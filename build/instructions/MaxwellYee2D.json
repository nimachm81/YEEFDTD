        "r1":[0.1, "_y1_", "_z1_"],
        "nCells":[1, "_ny_", "_nz_"],
        "dt":"_dt_"
    },
    "entireGridArrays":[{"name":"E", "type":"EdgeE"}, {"name":"H", "type":"EdgeH"}],
    "partialGridArrays":[{"name":"J", "type":"EdgeE", "indStart":[0, "_indyJ_", "_indzJ_"], "nCells":[1, 0, 0]}],
    "girdArrayManipulators":[
        {
        "type":"GaussianGridArrayManipulator",
        "parameters":{
