        {
        "type":"entire",
        "parameters":{
            "name":"H-y",
            "array":"H",
            "direction":"y",
            "saveRate":1
            }
        },
        {
        "type":"entire",
        "parameters":{
            "name":"J-x",
            "array":"J",
            "direction":"x",
            "saveRate":1
            }
        }
