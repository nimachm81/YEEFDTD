        },
        {
        "type":"A=frt",
        "parameters":{
            "name":"J_update",
            "girdArrayManipulator":"Jupdater"
            }
        }
    ],
    "updateSequences":[{"name":"iterative", "sequence":["J_update", "E_me_J", "E_pe_curl_H", "Hy_pe_m_curl_E", "Hz_pe_m_curl_E"]}],
    "runSequence":[{"name":"iterative", "timeIndStart":0, "timeIndEnd":"_nt_"}],
